# Copy this file to .env (same directory) and adjust values as needed for local dev.
# docker compose will automatically load variables from .env in the same folder.

# Postgres settings
POSTGRES_DB=knightmesh
POSTGRES_USER=km
POSTGRES_PASSWORD=kmPass!

# Keycloak admin user
KC_ADMIN=admin
KC_ADMIN_PASSWORD=admin

# Optional: image tag for locally built images
# Use with scripts/build-images.sh --tag <value>
IMAGE_TAG=dev

# Optional: OpenTelemetry endpoint (when using monitoring profile)
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317

# Spring profile propagated to app containers
SPRING_PROFILES_ACTIVE=dev
